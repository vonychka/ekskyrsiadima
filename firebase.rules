{
  "rules": {
    ".read": true,
    ".write": true,
    "tours": {
      ".read": true,
      ".write": true,
      ".indexOn": ["createdAt"],
      "$tourId": {
        ".read": true,
        ".write": true
      }
    },
    "schedules": {
      ".read": true,
      ".write": true,
      ".indexOn": ["createdAt", "tourId"],
      "$scheduleId": {
        ".read": true,
        ".write": true
      }
    },
    "bookings": {
      ".read": true,
      ".write": true,
      ".indexOn": ["createdAt", "tourId"],
      "$bookingId": {
        ".read": true,
        ".write": true
      }
    }
  }
}
